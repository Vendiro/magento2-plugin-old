{
    "name": "tig/vendiro-magento2",
    "description": "TIG Magento 2 Vendiro extension",
    "require": {
        "php": "~7.1|~7.2|~7.3",
        "magento/framework": "^101.0|^102.0"
    },
    "type": "magento2-module",
    "license": "CC-BY-NC-ND-3.0",
    "version": "1.1.1",
    "authors": [
        {
            "name": "TIG",
            "email": "support@tig.nl",
            "homepage": "https://tig.nl"
        },
        {
            "name": "Vendiro",
            "email": "support@vendiro.nl",
            "homepage": "https://vendiro.nl"
        }
    ],
    "support": {
        "email": "support@tig.nl",
        "issues": "https://portal.tig.nl/"
    },
    "homepage": "https://tig.nl/",
    "keywords": [
        "Vendiro",
        "Official Vendiro extension",
        "Magento 2"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "TIG\\Vendiro\\": ""
        }
    },
    "require-dev": {
        "object-calisthenics/phpcs-calisthenics-rules": "^2.0",
        "magento/marketplace-eqp": "^1.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    }
}
